<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Users & organizations{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('build/styles.css') }}">
            <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
        {% endblock %}

    </head>
    <body>
        <section id="wrapper">
            <header id="header">

                <hgroup>
                    <h2>{% block site_title %}<a href="{{ path('grt_homepage') }}">{% trans %}Users & Organizations {% endtrans %}</a>{% endblock %}</h2>
                </hgroup>
            </header>

            <section class="main-col">
                {% block javascripts %}
                    <script src="{{ asset('build/scripts.js') }}"></script>
                {% endblock %}
                {% block namepage %}{% endblock %}
                {% block body %}{% endblock %}
            </section>

            {% block sidebar %}
            {% endblock %}

            <div id="footer">
                {% block footer %}
                {% endblock %}
            </div>
        </section>

    </body>
</html>
